<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.suhail.loginattempt1.Activities.MainActivity">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/container"></FrameLayout>

    <android.support.design.widget.BottomNavigationView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:itemTextColor="@color/bnav_selector"
        android:id="@+id/bottom_nav_bar"
        app:itemBackground="@color/white"
        app:itemIconTint="@color/bnav_selector"
        android:elevation="2dp"
        android:layout_alignParentBottom="true"
        app:menu="@menu/bootom_nav_menu">
    </android.support.design.widget.BottomNavigationView>



</RelativeLayout>
